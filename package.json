{
  "name": "agilitywp",
  "version": "1.0.0",
  "description": "Agility WP Theme is a blazing fast theme for Bloggers, Business, WooCommerce, and Portfolio, etc. Build on the top of Bootstrap 5 with some amazing customization options best suited for any business.",
  "author": "LucenThemes",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "WordPress",
    "Theme"
  ],
  "homepage": "https://github.com/wayoutworks/awp#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:wayoutworks/awp.git"
  },
  "bugs": {
    "url": "https://github.com/wayoutworks/awp/issues"
  },
  "devDependencies": {
    "@wordpress/scripts": "^12.0.0",
    "grunt": "^1.2.1",
    "grunt-contrib-compress": "^1.6.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^3.0.0",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "rtlcss": "^2.5.0",
    "sass": "^1.26.10"
  },
  "rtlcssConfig": {
    "options": {
      "autoRename": false,
      "autoRenameStrict": false,
      "blacklist": {},
      "clean": true,
      "greedy": false,
      "processUrls": false,
      "stringMap": []
    },
    "plugins": [],
    "map": false
  },
  "scripts": {
    "compile:rtl": "rtlcss style.css style-rtl.css",
    "lint:scss": "wp-scripts lint-style 'resources/sass/**/*.scss'",
    "lint:js": "wp-scripts lint-js 'resources/js/*.js'"
  },
  "dependencies": {
    "@popperjs/core": "^2.4.4",
    "bootstrap": "^5.0.0-alpha1",
    "bootstrap-icons": "^1.0.0-alpha5"
  }
}
